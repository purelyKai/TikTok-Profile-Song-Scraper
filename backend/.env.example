# Backend Environment Variables (for CLI usage without Docker)
# Copy this file to .env and fill in your values

# Gemini API Key for AI song processing (required for --process)
GEMINI_API_KEY=your_gemini_api_key_here

# TikTok profile to scrape (optional, can use --profile flag instead)
PROFILE=username_to_scrape

# Run browser in headless mode (set to true for servers/CI)
HEADLESS=false
